<UserControl x:Class="CPSC_481_PROJECT.MainPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CPSC_481_PROJECT"
             mc:Ignorable="d" 
             d:DesignHeight="576" d:DesignWidth="1024">
    <Grid Background="White">
        <TabControl x:Name="MainPageTabControl" HorizontalAlignment="Left" Height="576" VerticalAlignment="Top" Width="1024">
            <TabItem x:Name="ProfileTab" Header="PROFILE" Width="510" Height="76">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem x:Name="SearchTab" Header="SEARCH" Width="510" Height="76">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</UserControl>
