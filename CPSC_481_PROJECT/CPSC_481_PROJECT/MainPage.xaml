                                <Image x:Name="friendImage1_Copy7" Source="Images/JUSTICE.png" HorizontalAlignment="Left" Margin="188.629,380,0,39" Width="162" Grid.Column="1"/>
                                <TextBlock x:Name="textBlock3" HorizontalAlignment="Left" Height="166" Margin="33.629,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Grid.Column="1"><Run Text="Games Played:"/><LineBreak/><Run Text="800"/></TextBlock>
